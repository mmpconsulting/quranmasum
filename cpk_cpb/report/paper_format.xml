<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="cpk_cpb.custom_paperformat" model="report.paperformat">
        <field name="name">Custom CPK CPB US Letter</field>
        <field name="default" eval="True"/>
        <field name="format">Letter</field>
        <field name="page_height">0</field>
        <field name="page_width">0</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">55</field>
        <field name="margin_bottom">10</field>
        <field name="margin_left">7</field>
        <field name="margin_right">7</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">50</field>
        <field name="dpi">90</field>
        <field name="report_ids" eval="[(6, 0, [ref('action_custom_report_pengolahan'), ref('action_custom_report_pengemasan')])]"/>
    </record>
</odoo>